{
  "name": "svg2woff2",
  "version": "0.3.2",
  "description": "generate woff2 font from svg files",
  "main": "dist/svg2woff2.js",
  "types": "dist/svg2woff2.d.ts",
  "author": {
    "name": "osawa naotaka"
  },
  "license": "MIT",
  "homepage": "https://github.com/osawa-naotaka/svg2woff2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osawa-naotaka/svg2woff2.git"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx ./app/app.ts",
    "analyze": "tsx ./app/analyze.ts",
    "watch": "tsx watch ./app/app.ts",
    "build": "tsc && tsup --config tsup.config.ts",
    "check": "biome check ./src ./types ./app",
    "check-write": "biome check --write ./src ./types ./app"
