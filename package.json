    "@types/svg2ttf": "^5.0.3",
    "opentype.js": "^1.3.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@lulliecat/svg2ttf": "^6.0.4",
    "element-to-path": "^1.2.1",
    "svg-path-commander": "^2.1.11",
    "svgson": "^5.3.1",
    "ttf2woff2": "^8.0.0"
